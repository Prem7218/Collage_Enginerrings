      <section id="webinar-details-4" class="webinar-details">
        <center>
            <p class="btns-1">Click and view</p>
            <span>Click above any webinar to get details about that Webinar and Register Link</span>
        </center>
      </section>
    </div>

    <script>
      function showDetails(webinarId) {
        document
          .querySelectorAll(".webinar-details")
          .forEach(function (section) {
            section.classList.remove("active");
          });

        document
          .getElementById("webinar-details-" + webinarId)
          .classList.add("active");

        document
          .getElementById("webinar-details-" + webinarId)
          .scrollIntoView({ behavior: "smooth" });
      }
    </script>
  </body>
